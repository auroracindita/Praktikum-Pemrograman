#include <bits/stdc++.h>
using namespace std;

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    int n;
    cin >> n;

    for (int i = 1; i <= 10; i++) {
        int cnt = n * i;  // Fix multiplication logic
        cout << n << " x " << i << " = " << cnt << '\n';  // Fix output operator
    }

    return 0;  // Return statement
}
